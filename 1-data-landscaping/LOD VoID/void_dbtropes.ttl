lod:dbtropes  a               void:Dataset ;
        dcterms:description   "DBTropes.org is a Linked Data wrapper for the TVTropes.org community wiki. It contains descriptions of numerous movies, books, and other items, and associates these with tropes (writing devices and conventions)."@en ;
        dcterms:publisher     [ rdfs:label  "TVTropes.org community" ;
                                foaf:mbox   ""
                              ] ;
        dcterms:subject       "published-by-third-party" , "license-metadata" , "media" , "format-rdf" , "deref-vocab" , "fiction" , "lodcloud-diagram-2014-08-30" , "lodcloud-diagram-2011-09-19" , "format-rdfs" , "movie" , "provenance-metadata" , "trope" , "book" , "lod" ;
        dcterms:title         "DBTropes"@en ;
        void:dataDump         <http://dbtropes.org/static/dbtropes.zip> ;
        void:exampleResource  <http://dbtropes.org/resource/Main/TheMatrix> ;
        void:triples          8000000 ;
        foaf:homepage         <http://dbtropes.org/> .
