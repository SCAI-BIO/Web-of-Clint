lod:event-media  a            void:Dataset ;
        dcterms:description   "This dataset is composed of events and media descriptions associated with these events. It is obtained from three large public event directories (last.fm, eventful and upcoming) represented with the LODE ontology and from large media directories (flickr, youtube) represented with the W3C Media Ontology. It is interlinked with the larger Linked Open Data cloud and in particular with DBpedia, Freebase, Geonames. It also contains links to numerous related web pages, including MusicBrainz, Geonames, Last.fm, Eventful, Upcoming and Foursquare.\r\n\r\n"@en ;
        dcterms:publisher     [ rdfs:label  "Raphael Troncy" ;
                                foaf:mbox   "raphael.troncy@eurecom.fr"
                              ] ;
        dcterms:subject       "media" , "format-ma" , "published-by-third-party" , "provenance-metadata" , "deref-vocab" , "format-rdf" , "format-lode" , "format-sioc" , "format-dc" , "no-license-metadata" , "events" , "usergeneratedcontent" , "format-foaf" , "vocab-mappings" , "lod" ;
        dcterms:title         "EventMedia"@en ;
