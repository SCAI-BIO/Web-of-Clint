lod:nvs  a                    void:Dataset ;
        dcterms:description   "The NERC Vocabulary Server provides access to lists of standardised terms that cover a broad spectrum of disciplines of relevance to the oceanographic and wider community.\r\n\r\nUsing standardised sets of terms (otherwise known as \"controlled vocabularies\") in metadata and to label data solves the problem of ambiguities associated with data markup and also enables records to be interpreted by computers. This opens up data sets to a whole world of possibilities for computer aided manipulation, distribution and long term reuse.\r\n\r\nAll of the vocabularies are fully versioned and a permanent record is kept of all changes made."@en ;
        dcterms:publisher     [ rdfs:label  "British Oceanographic Data Centre, Natural Environment Research Council, UK" ;
                                foaf:mbox   "enquiries@bodc.ac.uk"
                              ] ;
        dcterms:subject       "ocean" , "life_sciences" , "format-dc" , "schemata" , "format-rdfs" , "vocabulary" , "published-by-producer" , "controlled-vocabulary" , "earth-science" , "no-proprietary-vocab" , "lod" , "environment" , "provenance-metadata" , "format-skos" , "lodcloud-diagram-2014-08-30" , "format-rdf" , "no-license-metadata" , "format-owl" , "oceanography" , "oceans" ;
        dcterms:title         "NERC Vocabulary Server 2.0"@en ;
