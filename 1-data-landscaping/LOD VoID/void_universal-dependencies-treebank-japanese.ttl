lod:universal-dependencies-treebank-japanese
        a                    void:Dataset ;
        dcterms:description  "Universal Dependencies is a project that seeks to develop cross-linguistically consistent treebank annotation for many languages, with the goal of facilitating multilingual parser development, cross-lingual learning, and parsing research from a language typology perspective. The annotation scheme is based on (universal) Stanford dependencies (de Marneffe et al., 2006, 2008, 2014), Google universal part-of-speech tags (Petrov et al., 2012), and the Interset interlingua for morphosyntactic tagsets (Zeman, 2008).\r\n\r\nThis dataset refers to UD's Japanese treebank.\r\n"@en ;
        dcterms:publisher    [ rdfs:label  "McDonald, Ryan; Nivre, Joakim; Zeman, Daniel" ;
                               foaf:mbox   "zeman@ufal.mff.cuni.cz"
                             ] ;
        dcterms:subject      "lod" , "linguistics" , "corpus" ;
        dcterms:title        "Universal Dependencies Treebank Japanese"@en ;
        void:triples         92033 ;
        dcat:distribution    [ dcat:accessURL  <https://github.com/UniversalDependencies/UD_Japanese/archive/master.zip> ] ;
        dcat:distribution    [ dcat:accessURL  <https://datahub.ckan.io/dataset/671feab9-719a-47d1-95fc-bf08e33c61b2/resource/3b7028f4-b3e6-48be-8c96-30f226055216/download/udjapanese.rdf> ] ;
        foaf:homepage        <https://github.com/UniversalDependencies/UD_Japanese> .
