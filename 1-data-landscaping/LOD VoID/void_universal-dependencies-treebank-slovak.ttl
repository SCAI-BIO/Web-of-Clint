lod:universal-dependencies-treebank-slovak
        a                    void:Dataset ;
        dcterms:description  "Universal Dependencies is a project that seeks to develop cross-linguistically consistent treebank annotation for many languages, with the goal of facilitating multilingual parser development, cross-lingual learning, and parsing research from a language typology perspective. The annotation scheme is based on (universal) Stanford dependencies (de Marneffe et al., 2006, 2008, 2014), Google universal part-of-speech tags (Petrov et al., 2012), and the Interset interlingua for morphosyntactic tagsets (Zeman, 2008).\r\n\r\nThis dataset refers to UD's Slovak treebank.\r\n"@en ;
        dcterms:publisher    [ rdfs:label  "Gajdošová, Katarína; Šimková, Mária; Zeman, Daniel" ;
                               foaf:mbox   "zeman@ufal.mff.cuni.cz"
                             ] ;
        dcterms:subject      "lod" , "linguistics" , "corpus" ;
        dcterms:title        "Universal Dependencies Treebank Slovak"@en ;
        void:triples         106043 ;
        dcat:distribution    [ dcat:accessURL  <https://datahub.ckan.io/dataset/b19be93d-9fee-4043-908d-ca8eb34b9e0e/resource/d897eae0-7154-4348-962d-4b37f81528f3/download/udslovak.rdf> ] ;
